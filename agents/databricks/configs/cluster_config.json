{
  "cluster_name": "agents-cluster",
  "spark_version": "13.3.x-scala2.12",
  "node_type_id": "i3.xlarge",
  "driver_node_type_id": "i3.xlarge",
  "num_workers": 2,
  "autoscale": {
    "min_workers": 1,
    "max_workers": 4
  },
  "spark_conf": {
    "spark.databricks.cluster.profile": "serverless",
    "spark.databricks.repl.allowedLanguages": "python,sql"
  },
  "custom_tags": {
    "project": "langgraph-agents",
    "environment": "development"
  },
  "spark_env_vars": {
    "PYSPARK_PYTHON": "/databricks/python3/bin/python3"
  },
  "init_scripts": [
    {
      "dbfs": {
        "destination": "dbfs:/databricks/init-scripts/install-agents-deps.sh"
      }
    }
  ],
  "libraries": [
    {
      "pypi": {
        "package": "langgraph>=0.2.0"
      }
    },
    {
      "pypi": {
        "package": "langchain>=0.3.0"
      }
    },
    {
      "pypi": {
        "package": "langchain-openai>=0.2.0"
      }
    },
    {
      "pypi": {
        "package": "langchain-community>=0.3.0"
      }
    },
    {
      "pypi": {
        "package": "tavily-python>=0.3.0"
      }
    },
    {
      "pypi": {
        "package": "pydantic>=2.5.0"
      }
    }
  ],
  "enable_elastic_disk": true,
  "data_security_mode": "USER_ISOLATION",
  "runtime_engine": "STANDARD"
}

